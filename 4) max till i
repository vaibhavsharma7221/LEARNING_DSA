//max till i       

#include<bits/stdc++.h>
using namespace std;

int main()
{
    int s,i;
    cout<<"enter size of array :";
    cin>>s;
    int arr[s];
    for(i=0;i<s;i++)
    {
        cin>>arr[i];
    }
    
   int maxi=INT_MIN;
    
    for(i=0;i<s;i++)         //or    for(i=0;i<s;i++)  
    {                        //      {
        if(arr[i]>maxi)      //      maxi=max(maxi,arr[i]);
        {                    //      cout<<maxi<<" ";
        maxi=arr[i];         //      }
        }                    //      return 0;
        arr[i]=maxi;         //      }
        cout<<arr[i]<<" ";
    }
    
    return 0;
}
